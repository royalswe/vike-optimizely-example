{
  "name": "optimizely-skeleton-vue3",
  "version": "1.0.0-beta.2",
  "main": "dist/server.js",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node --esm ./server/server.ts",
    "dev:debug": "node --inspect -r ts-node/register/transpile-only --loader ts-node/esm ./server/server.ts",
    "dev:watch": "nodemon server -e ts,json --exec \"npm run dev\"",
    "build": "vite build && npm run move-to-dist && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --build ./server",
    "build:dev": "npm run build:client -- -m development && npm run build:server && npm run postbuild && npm i --prefix dist --omit=dev",
    "preview": "cross-env NODE_ENV=production NODE_OPTIONS='--inspect' IS_AZURE=false API_BASE_URL=https://localhost.com node ./dist/server.js",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "move-to-dist": "mkdir temp && move /y .//dist .//temp//dist && ren temp dist",
    "postbuild": "copy package.json dist\\package.json && copy .\\server\\run.cjs dist\\run.cjs && copy .\\server\\web.config dist\\web.config"
  },
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@azure/keyvault-secrets": "^4.7.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "applicationinsights": "^2.9.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lazysizes": "^5.3.2",
    "npm": "^10.2.5",
    "pinia": "^2.1.7",
    "sirv": "^2.0.4",
    "vike": "^0.4.152",
    "vue": "^3.4.x",
    "vue-cookies": "^1.8.3",
    "vue-i18n": "^9.8.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "sass": "^1.56.1",
    "sass-loader": "^12.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-mkcert": "^1.17.1",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": "18.12.1"
  },
  "type": "module"
}
