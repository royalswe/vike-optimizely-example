{
  "name": "optimizely-skeleton-vue3",
  "version": "1.0.0-beta.2",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx ./server/server.ts",
    "dev:debug": "node --inspect -r ts-node/register/transpile-only --loader ts-node/esm ./server/server.ts",
    "dev:watch": "nodemon server -e ts,json --exec \"npm run dev\"",
    "build": "vite build && npm run move-to-dist && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --build ./server",
    "build:dev": "npm run build:client -- -m development && npm run build:server && npm run postbuild && npm i --prefix dist --omit=dev",
    "preview": "cross-env NODE_ENV=production NODE_OPTIONS='--inspect' IS_AZURE=false API_BASE_URL=https://localhost.com node ./dist/server.js",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "move-to-dist": "mkdir temp && move /y .//dist .//temp//dist && ren temp dist",
    "postbuild": "copy package.json dist\\package.json && copy .\\server\\run.cjs dist\\run.cjs && copy .\\server\\web.config dist\\web.config"
  },
  "dependencies": {
    "@fastify/compress": "^7.0.0",
    "@fastify/static": "^7.0.1",
    "@vitejs/plugin-vue": "^5.0.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.4",
    "fastify": "^4.26.1",
    "lazysizes": "^5.3.2",
    "pinia": "^2.1.7",
    "vike": "^0.4.163",
    "vue": "^3.4.20",
    "vue-cookies": "^1.8.3",
    "vue-i18n": "^9.8.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.0",
    "@types/node": "^20.11.20",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "msw": "^2.0.11",
    "sass": "^1.56.1",
    "sass-loader": "^12.6.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-mkcert": "^1.17.3",
    "vue-tsc": "^1.8.27"
  },
  "type": "module"
}
